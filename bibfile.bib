@article{silverstone_qubit_2014,
	title = {Qubit entanglement on a silicon photonic chip},
	url = {http://arxiv.org/abs/1410.8332},
	abstract = {Entanglement--one of the most delicate phenomena in nature--is an essential resource for quantum information applications. Large entangled states have been theoretically shown to allow universal quantum computation, with a model based on single-qubit measurements of a cluster state being particularly amenable to an implementation with photons. Useful large-scale quantum devices must be able to generate and control qubit entanglement on-chip, where quantum information is naturally encoded in the photonic path degree of freedom. Here we report a silicon photonic chip which integrates resonant-enhanced sources, filters, and reconfigurable optics to generate a path-entangled two-qubit state--the smallest non-trivial cluster state--and to coherently analyse its entanglement on-chip. We show that two ring-resonator-based spontaneous four-wave mixing (SFWM) sources can be made highly indistinguishable, despite their well-documented nonlinear dynamics, and we show the first evidence that their frequency correlations are small, as predicted. We use quantum state tomography, and the strict Bell-CHSH inequality violation to quantify the entanglement present in the device, and use that entanglement as an indicator of device performance. We find a 91+/-1\% overlap with a maximally entangled state, and a Bell-CHSH violation of 2.69+/-0.03, demonstrating a very high level of entanglement. Our quantum photonic device is the most functionally complex ever: it integrates components which will be invaluable for building devices and systems to harness quantum entanglement on the large scale.},
	urldate = {2014-11-10},
	journal = {arXiv:1410.8332 [quant-ph]},
	author = {Silverstone, Joshua W. and Santagati, Raffaele and Bonneau, Damien and Strain, Michael J. and Sorel, Marc and O'Brien, Jeremy L. and Thompson, Mark G.},
	month = oct,
	year = {2014},
	note = {arXiv: 1410.8332},
	keywords = {Quantum Physics},
	file = {arXiv\:1410.8332 PDF:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/9JRE3BS4/Silverstone et al. - 2014 - Qubit entanglement on a silicon photonic chip.pdf:application/pdf;arXiv.org Snapshot:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/QHT7KSMA/1410.html:text/html}
}

