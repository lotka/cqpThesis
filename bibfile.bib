	pages = {2199--2202},
	file = {Full Text PDF:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/N2BQEK3S/Ensslin et al. - 2010 - A quantum mechanics lab on a chip.pdf:application/pdf;Snapshot:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/URTBUZFE/Ensslin et al. - 2010 - A quantum mechanics lab on a chip.html:text/html}
}

@article{engin_photon_2012,
	title = {Photon {Pair} {Generation} in {Silicon} {Micro}-{Ring} {Resonator} with {Reverse} {Bias} {Enhancement}},
	url = {http://arxiv.org/abs/1204.4922},
	abstract = {Photon sources are fundamental components for any quantum photonic technology. The ability to generate high count-rate and low-noise correlated photon pairs via spontaneous parametric down-conversion using bulk crystals has been the cornerstone of modern quantum optics. However, future practical quantum technologies will require a scalable integration approach, and waveguide-based photon sources with high-count rate and low-noise characteristics will be an essential part of chip-based quantum technologies. Here, we demonstrate photon pair generation through spontaneous four-wave mixing in a silicon micro-ring resonator, reporting a maximum coincidence-to-accidental (CAR) ratio of 602 (+-) 37, and a maximum photon pair generation rate of 123 MHz (+-) 11 KHz. To overcome free-carrier related performance degradations we have investigated reverse biased p-i-n structures, demonstrating an improvement in the pair generation rate by a factor of up to 2, with negligible impact on CAR.},
	urldate = {2014-11-18},
	journal = {arXiv:1204.4922 [physics, physics:quant-ph]},
	author = {Engin, Erman and Bonneau, Damien and Natarajan, Chandra M. and Clark, Alex and Tanner, M. G. and Hadfield, R. H. and Dorenbos, Sanders N. and Zwiller, Val and Ohira, Kazuya and Suzuki, Nobuo and Yoshida, Haruhiko and Iizuka, Norio and Ezaki, Mizunori and O'Brien, Jeremy L. and Thompson, Mark G.},
