
@article{toth_quantum_2014,
	title = {Quantum metrology from a quantum information science perspective},
	volume = {47},
	issn = {1751-8113, 1751-8121},
	url = {http://arxiv.org/abs/1405.4878},
	doi = {10.1088/1751-8113/47/42/424006},
	abstract = {We summarise important recent advances in quantum metrology, in connection to experiments in cold gases, trapped cold atoms and photons. First we review simple metrological setups, such as quantum metrology with spin squeezed states, with Greenberger-Horne-Zeilinger states, Dicke states and singlet states. We calculate the highest precision achievable in these schemes. Then, we present the fundamental notions of quantum metrology, such as shot-noise scaling, Heisenberg scaling, the quantum Fisher information and the Cramer-Rao bound. Using these, we demonstrate that entanglement is needed to surpass the shot-noise scaling in very general metrological tasks with a linear interferometer. We discuss some applications of the quantum Fisher information, such as how it can be used to obtain a criterion for a quantum state to be a macroscopic superposition. We show how it is related to the the speed of a quantum evolution, and how it appears in the theory of the quantum Zeno effect. Finally, we explain how uncorrelated noise limits the highest achievable precision in very general metrological tasks.},
	number = {42},
	urldate = {2015-04-20},
	journal = {Journal of Physics A: Mathematical and Theoretical},
	author = {Toth, Geza and Apellaniz, Iagoba},
	month = oct,
	year = {2014},
	note = {arXiv: 1405.4878},
	keywords = {Condensed Matter - Quantum Gases, Physics - Atomic Physics, Quantum Physics},
	pages = {424006},
	file = {arXiv\:1405.4878 PDF:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/32R8S597/Toth and Apellaniz - 2014 - Quantum metrology from a quantum information scien.pdf:application/pdf;arXiv.org Snapshot:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/WZCBP7UE/1405.html:text/html}
}

@article{shor_polynomial-time_1995,
	title = {Polynomial-{Time} {Algorithms} for {Prime} {Factorization} and {Discrete} {Logarithms} on a {Quantum} {Computer}},
	url = {http://arxiv.org/abs/quant-ph/9508027},
	abstract = {A digital computer is generally believed to be an efficient universal computing device; that is, it is believed able to simulate any physical computing device with an increase in computation time of at most a polynomial factor. This may not be true when quantum mechanics is taken into consideration. This paper considers factoring integers and finding discrete logarithms, two problems which are generally thought to be hard on a classical computer and have been used as the basis of several proposed cryptosystems. Efficient randomized algorithms are given for these two problems on a hypothetical quantum computer. These algorithms take a number of steps polynomial in the input size, e.g., the number of digits of the integer to be factored.},
	urldate = {2015-04-20},
	journal = {arXiv:quant-ph/9508027},
	author = {Shor, Peter W.},
	month = aug,
	year = {1995},
	note = {arXiv: quant-ph/9508027},
	keywords = {Quantum Physics},
	file = {arXiv.org Snapshot:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/GRD87MTI/9508027.html:text/html;arXiv\:quant-ph/9508027 PDF:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/9GEWD67J/Shor - 1995 - Polynomial-Time Algorithms for Prime Factorization.pdf:application/pdf}
