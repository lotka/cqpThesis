	keywords = {nonlinear optics, Photon pair sources, semiconductor photonics, spectral correlations},
	pages = {L76--L80},
	file = {Snapshot:/home/luka/.zotero/zotero/06u0dmyi.default/zotero/storage/TFE8SF2F/abstract.html:text/html}
}

